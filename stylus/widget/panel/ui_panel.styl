@import "../../base/fn"


.ui-panel
	width 100%
	padding panelPadding
	background-color panelBgColor
	border-radius panelBorderRadius
	display:block
	box-sizing border-box
	overflow hidden

.ui-panel + .ui-panel
	margin-top panelMarginTop

.ui-panel-inline
	width auto
	display inline-block
	margin-left panelMarginLeft
	margin-top auto
	